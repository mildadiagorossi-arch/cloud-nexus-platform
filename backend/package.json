{
    "name": "backend",
    "version": "1.0.0",
    "private": true,
    "type": "commonjs",
    "scripts": {
        "start": "ts-node -P tsconfig.json src/main.ts",
        "start:dev": "ts-node -P tsconfig.json src/main.ts",
        "build": "tsc -p tsconfig.json",
        "seed": "ts-node src/seed.ts",
        "seed:reset": "prisma db push --schema=src/infrastructure/persistence/prisma/schema.prisma --force-reset && npm run seed"
    },
    "dependencies": {
        "@nestjs/common": "^10.0.0",
        "@nestjs/core": "^10.0.0",
        "@nestjs/platform-express": "^10.0.0",
        "@nestjs/platform-socket.io": "^11.1.11",
        "@nestjs/websockets": "^11.1.11",
        "@prisma/client": "5.22.0",
        "@types/bcryptjs": "^2.4.6",
        "@types/passport-jwt": "^4.0.1",
        "passport-google-oauth20": "^2.0.0",
        "passport-jwt": "^4.0.1",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.8.1",
        "socket.io": "^4.8.3"
    },
    "devDependencies": {
        "@nestjs/cli": "^11.0.14",
        "@types/node": "^20.3.1",
        "@types/passport-google-oauth20": "^2.0.17",
        "prisma": "5.22.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.3"
    }
}
